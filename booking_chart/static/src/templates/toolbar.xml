<template>

    <t t-name="Booking.Toolbar">
        <div class="booking-toolbar">
            <button type="button" class="btn_today oe_highlight">
                Today
            </button>
            <button type="button" class="btn_freeze">
                Freeze
            </button>
            <div class="rangepicker" style="text-align:center;">
                <label for="from">From</label>
                <input type="text" t-attf-id="date_picker_from" t-att-value="from" name="from" readonly="readonly"/>
                <label for="to">to</label>
                <input type="text" t-attf-id="date_picker_to" t-att-value="to" name="to" readonly="readonly"/>
                <button type="button" id="btnShow">
                    Show
                </button>
            </div>
        </div>
    </t>

</template>